          "_customHTML": "<section data-bs-version=\"5.1\" class=\"footer01 modelm5\" group=\"Footer\" global once=\"footers\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Logo\" name=\"showLogo\" checked>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" condition=\"showNav || showContacts\" checked>\n        <input type=\"checkbox\" title=\"Navigation\" name=\"showNav\" checked>\n        <select title=\"Amount\" name=\"list\" condition=\"showNav\">\n            <option value=\"1\">1</option>\n            <option value=\"2\" selected>2</option>&gt;\n        </select>\n        <input type=\"checkbox\" title=\"Contacts\" name=\"showContacts\">\n        <input type=\"checkbox\" title=\"Address\" name=\"showAddress\" condition=\"showContacts\" checked>\n        <input type=\"checkbox\" title=\"Contacts List\" name=\"showContList\" condition=\"showContacts\" checked>\n        <input type=\"color\" title=\"Active Color\" value=\"#71EAC6\" name=\"active\" condition=\"showNav || showContacts\" selected>\n        <input type=\"checkbox\" title=\"Border\" name=\"showBorder\" checked>\n        <input type=\"color\" title=\"Border Color\" value=\"#BEE0D6\" name=\"border\" condition=\"showBorder\" selected>\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopy\" checked>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#27232C\" selected>\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12 col-lg-6\" mbr-if=\"showLogo || showTitle\">\n                <div class=\"content-wrapper\">\n                    <div class=\"logo-wrapper\" mbr-if=\"showLogo\">\n                        <a href=\"#\"><img src=\"@PROJECT_PATH@/assets/images/pitzerdev-3-128x128.png\" alt=\"Mobirise\"></a>\n                    </div>\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .logo-wrapper\">PitzerDEV<br>Agency</h2>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-2\" mbr-if=\"showNav\">\n                <div class=\"nav-wrapper\" mbr-if=\"list > 1\">\n                    <h3 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">\n                        Menu\n                    </h3>\n                    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list, .item-wrap, .nav-wrapper\" data-multiline mbr-article>\n                        <li class=\"item-wrap\"><a href=\"index.html#header02-1\" class=\"text-primary\">Home</a></li>\n                        <li class=\"item-wrap\"><a href=\"index.html#article01-5\" class=\"text-primary\">Sobre</a></li>\n                        <li class=\"item-wrap\"><a href=\"index.html#slider02-4\" class=\"text-primary\">Portfólio</a></li>\n                        <li class=\"item-wrap\"><br></li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-2\" mbr-if=\"showNav\">\n                <div class=\"nav-wrapper\">\n                    <h3 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">\n                        Social\n                    </h3>\n                    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list, .item-wrap, .nav-wrapper\" data-multiline mbr-article>\n                        <li class=\"item-wrap\"><a href=\"https://www.instagram.com/dezzani_gabriel/\" class=\"text-primary\">Instagram</a></li>\n                        <li class=\"item-wrap\"><a href=\"mailto:gabrielpitzer@hotmail.com\" class=\"text-primary\">e-Mail</a></li><a href=\"mailto:gabrielpitzer@hotmail.com\" class=\"text-primary\">\n                        </a><li class=\"item-wrap\"><a href=\"https://wa.me/5532998108311\" class=\"text-primary\">Whatsapp&nbsp;</a></li><a href=\"https